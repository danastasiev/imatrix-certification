{
  "name": "imatrix-cert-backend",
  "version": "0.0.0",
  "private": true,
  "main": "build/src/app.js",
  "scripts": {
    "build": "tsc && npm run copy:test-migrations && npm run copy:migrations",
    "start": "node ./build/index.js",
    "copy:migrations": "copyfiles -f ./src/db/migrations/*.sql ./build/db/migrations",
    "copy:test-migrations": "copyfiles -f ./src/db/test-migrations/*.sql ./build/db/test-migrations",
    "test": "node --expose-gc ./node_modules/jest-cli/bin/jest.js --testPathPattern=\".*\\.inttest\\.ts\" --runInBand --forceExit --detectOpenHandles --logHeapUsage"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.12.21",
    "@types/uuid": "^7.0.2",
    "@types/joi": "^14.3.2",
    "@types/cors": "^2.8.6",
    "@types/jest": "^24.0.11",
    "@types/js-sha512": "^0.7.0",
    "@types/json2csv": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "copyfiles": "^2.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "prettier": "1.17.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.0",
    "ts-jest": "^24.0.1",
    "tslint": "^5.20.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "typedi": "^0.8.0",
    "knex": "^0.20.11",
    "mysql": "^2.18.1",
    "uuid": "^7.0.3",
    "js-sha512": "^0.8.0",
    "axios": "^0.19.0",
    "joi": "^14.3.1",
    "openssl-nodejs": "^1.0.5",
    "winston": "^3.0.0",
    "express-winston": "^4.0.4",
    "cors": "^2.8.4",
    "json2csv": "^5.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>src/test/setup-tests.ts"
    ],
    "testRegex": "(/(src)/(test)/.*(\\.|/)(inttest))\\.(js|ts)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
